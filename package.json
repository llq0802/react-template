{
  "name": "base-template",
  "version": "1.0.0",
  "private": true,
  "description": "A React Demo With Ant Design",
  "scripts": {
    "start": "cross-env REACT_APP_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "build": "umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "test": "umi test",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "prettier": "prettier -c --write \"src/**/*\"",
    "prepare": "husky install",
    "postinstall": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ],
    "src/**/*.less": [
      "stylelint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.1.20",
    "@ant-design/icons": "^4.3.0",
    "@ccs-design/rc-pro": "1.0.6-beta-7",
    "ahooks": "^2.9.6",
    "antd": "^4.16.13",
    "bizcharts": "^4.0.17",
    "braft-editor": "^2.2.10",
    "classnames": "^2.2.6",
    "draftjs-to-html": "^0.8.4",
    "html-to-draftjs": "^1.4.0",
    "immutability-helper": "^3.1.1",
    "insert-css": "^2.0.0",
    "js-base64": "^2.5.1",
    "jsencrypt": "^3.0.0-rc.1",
    "localforage": "^1.9.0",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "nprogress": "^0.2.0",
    "qs": "^6.9.0",
    "react": "^16.8.6",
    "react-container-query": "^0.11.0",
    "react-contextmenu": "^2.13.0",
    "react-dom": "^16.8.6",
    "react-draft-wysiwyg": "^1.13.2",
    "react-media-hook2": "^1.1.2",
    "react-text-loop": "^2.3.0",
    "umi": "3.5.16",
    "umi-request": "^1.2.9",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.8.4",
    "@umijs/fabric": "2.3.0",
    "@umijs/plugin-antd": "^0.5.1",
    "@umijs/plugin-dva": "^0.13.0",
    "@umijs/plugin-esbuild": "^1.3.1",
    "@umijs/yorkie": "^2.0.3",
    "cross-env": "^7.0.3",
    "mockjs": "^1.0.1-beta3",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.15.0",
    "prettier": "^2.3.1",
    "stylelint": "13.0.0",
    "typescript": "^4.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^10.0.0",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "license": "MIT"
}